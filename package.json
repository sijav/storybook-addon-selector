{
  "name": "@sijav/storybook-addon-selector",
  "version": "5.2.8-1",
  "description": "A storybook addon to have selector for your preview",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "format:check": "prettier --check '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "lint": "eslint --max-warnings=0 src/**/*.{ts,tsx}",
    "pre-commit": "lint-staged && yarn lint",
    "prepublishOnly": "yarn build",
    "prebuild": "yarn clean",
    "build": "tsc"
  },
  "dependencies": {
    "@storybook/addons": "5.2.8",
    "@storybook/api": "5.2.8",
    "@storybook/components": "5.2.8",
    "react": "^16.8.3",
    "util-deprecate": "^1.0.2"
  },
  "devDependencies": {
    "@types/util-deprecate": "^1.0.0",
    "@types/webpack-env": "^1.14.1",
    "eslint": "^6.3.0",
    "eslint-plugin-react-app": "^6.0.1",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}